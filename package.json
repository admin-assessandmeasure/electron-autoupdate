{
  "name": "infoapto",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dist": "electron-builder",
    "build:prod": "electron-builder --win",
    "build:publish": "set GH_TOKEN=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 && electron-builder --win  --publish always"
  },
  "keywords": [],
  "author": "Infoapto",
  "license": "ISC",
  "description": "File upload app",
  "build": {
    "appId": "com.infoapto.demo",
    "productName": "Infoapto",
    "icon": "./appicons/icons/win/infoaptoicon.png",
    "directories": {
      "output": "electron-build",
      "buildResources": "build-assets"
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "extraResources": [
      {
        "from": "./appicons/icons/win",
        "to": "."
      }
    ]
  },
  "repository": "https://github.com/admin-assessandmeasure/electron-autoupdate",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.0",
    "electron-builder": "^23.6.0",
    "electron-prebuilt-compile": "^8.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./appicons/icons/win/infoaptoicon.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "config": {
            "name": "recrubot"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "async": "^3.2.4",
    "auto-launch": "^5.0.5",
    "aws-sdk": "^2.855.0",
    "axios": "^1.1.3",
    "create-desktop-shortcuts": "^1.10.1",
    "dotenv": "^16.0.3",
    "electron-forge": "^5.2.4",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-storage": "^1.0.7",
    "electron-updater": "^5.3.0",
    "git": "^0.1.5",
    "https": "^1.0.0",
    "jwt-decode": "^3.1.2",
    "keyword-extractor": "^0.0.23",
    "mime": "^2.5.2",
    "moment": "^2.29.4",
    "mousetrap": "^1.6.5",
    "mv": "^2.1.1",
    "pdf-parse": "^1.1.1",
    "pdf-text-extract": "^1.5.0",
    "python-shell": "^3.0.1",
    "textract": "^2.5.0",
    "underscore": "^1.13.6",
    "word-extractor": "^1.0.4"
  }
}
